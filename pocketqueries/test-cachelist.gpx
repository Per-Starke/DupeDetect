    <sym>Geocache Found</sym>
    <type>Geocache|Virtual Cache</type>
    <groundspeak:cache id="34571" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Gem of Big Sur</groundspeak:name>
      <groundspeak:placed_by>Team Jelly Belly &amp; Leap Frog</groundspeak:placed_by>
      <groundspeak:owner id="38178">"Flintstones"</groundspeak:owner>
