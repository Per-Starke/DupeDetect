    <groundspeak:cache id="1845443" available="False" archived="True" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Das Geheimnis der Heubergindianer</groundspeak:name>
      <groundspeak:placed_by>wjojow</groundspeak:placed_by>
      <groundspeak:owner id="2883755">wjojow</groundspeak:owner>
      <groundspeak:type>Multi-cache</groundspeak:type>
