    <groundspeak:cache id="115864" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>The heart of munich</groundspeak:name>
      <groundspeak:placed_by>Windi</groundspeak:placed_by>
      <groundspeak:owner id="131379">Windi</groundspeak:owner>
      <groundspeak:type>Webcam Cache</groundspeak:type>
