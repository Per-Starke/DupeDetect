    <groundspeak:cache id="680649" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Oans, zwoa - g´funda!</groundspeak:name>
      <groundspeak:placed_by>~ Toasty ~</groundspeak:placed_by>
      <groundspeak:owner id="1210591">~ Toasty ~</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
      <groundspeak:container>Small</groundspeak:container>
      <groundspeak:difficulty>1</groundspeak:difficulty>
