    <url>http://www.geocaching.com/seek/cache_details.aspx?guid=66add4c6-d98a-4115-8464-16147de45d78</url>
    <urlname>Hinterlassenschaften eines Gletschers</urlname>
    <sym>Geocache Found</sym>
    <type>Geocache|Earthcache</type>
    <groundspeak:cache id="1598512" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Hinterlassenschaften eines Gletschers</groundspeak:name>
      <groundspeak:placed_by>Thoto</groundspeak:placed_by>
      <groundspeak:owner id="328414">Thoto</groundspeak:owner>
