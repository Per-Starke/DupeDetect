    <groundspeak:cache id="2931551" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Serie "La sombra del viento": Els quatre gats</groundspeak:name>
      <groundspeak:placed_by>Equip 36</groundspeak:placed_by>
