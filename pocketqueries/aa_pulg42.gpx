    <url>http://www.geocaching.com/seek/cache_details.aspx?guid=c90c3798-8f31-4600-895e-9f466fbdd8b1</url>
    <urlname>Heroes' Square - City Park Tour No. 1.</urlname>
    <sym>Geocache Found</sym>
    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="1240426" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Heroes' Square - City Park Tour No. 1.</groundspeak:name>
      <groundspeak:placed_by>ramgab</groundspeak:placed_by>
      <groundspeak:owner id="1850237">ramgab</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
      <groundspeak:container>Small</groundspeak:container>
