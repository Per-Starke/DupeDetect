    <groundspeak:cache id="1601535" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Denkmal der Petersbergbahn</groundspeak:name>
      <groundspeak:placed_by>skulll</groundspeak:placed_by>
