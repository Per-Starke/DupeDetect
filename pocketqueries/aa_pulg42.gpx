    <groundspeak:cache id="2692005" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Zum Venedigli</groundspeak:name>
      <groundspeak:placed_by>SR4ever</groundspeak:placed_by>
      <groundspeak:owner id="4402639">SR4ever</groundspeak:owner>
