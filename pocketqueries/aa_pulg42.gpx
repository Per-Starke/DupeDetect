    <groundspeak:cache id="1012615" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Geolehrpfad Winkelberg</groundspeak:name>
      <groundspeak:placed_by>MiTiKa &amp; Samba</groundspeak:placed_by>
      <groundspeak:owner id="1049720">Team MiTiKa</groundspeak:owner>
      <groundspeak:type>Earthcache</groundspeak:type>
