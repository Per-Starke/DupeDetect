    <groundspeak:cache id="324995" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Der Adventskalender an der Prießnitz: 21. Dezember</groundspeak:name>
      <groundspeak:placed_by>willimax</groundspeak:placed_by>
      <groundspeak:owner id="260309">willimax</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
