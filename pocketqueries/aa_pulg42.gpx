    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="1798650" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>One Tree on The Planet</groundspeak:name>
