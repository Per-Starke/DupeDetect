    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="2011260" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Kling Glöckchen - Der Cache</groundspeak:name>
      <groundspeak:placed_by>Cologne Geckos &amp; Rheinflut1995</groundspeak:placed_by>
      <groundspeak:owner id="1203091">rheinflut1995</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
      <groundspeak:container>Regular</groundspeak:container>
      <groundspeak:difficulty>2</groundspeak:difficulty>
