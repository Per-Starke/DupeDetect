    <groundspeak:cache id="2907505" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>EKW-Kapelle</groundspeak:name>
      <groundspeak:placed_by>Mangima</groundspeak:placed_by>
      <groundspeak:owner id="948898">mangima</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
