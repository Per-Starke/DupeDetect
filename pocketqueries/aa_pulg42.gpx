    <groundspeak:cache id="330850" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>DaunHill</groundspeak:name>
      <groundspeak:placed_by>Raumoko</groundspeak:placed_by>
      <groundspeak:owner id="282770">Raumoko</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
