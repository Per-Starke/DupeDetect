    <groundspeak:cache id="422275" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Hauptbahnhof Dresden</groundspeak:name>
      <groundspeak:placed_by>ToKer, feustel</groundspeak:placed_by>
      <groundspeak:owner id="682397">feustel</groundspeak:owner>
