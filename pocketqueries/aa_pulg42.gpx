    <groundspeak:cache id="1011599" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Loseeville</groundspeak:name>
      <groundspeak:placed_by>AD0OR</groundspeak:placed_by>
      <groundspeak:owner id="42884">JAC0B</groundspeak:owner>
