      <groundspeak:name>Gletschermühle</groundspeak:name>
      <groundspeak:placed_by>Steinlaus71</groundspeak:placed_by>
      <groundspeak:owner id="3513132">Steinlaus71</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
