    <sym>Geocache Found</sym>
    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="893433" available="False" archived="True" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Historische Spuren in Köln: Sportpark Müngersdorf</groundspeak:name>
      <groundspeak:placed_by>Thoto</groundspeak:placed_by>
      <groundspeak:owner id="328414">Thoto</groundspeak:owner>
