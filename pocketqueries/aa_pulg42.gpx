    <groundspeak:cache id="1658264" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Wahn-Sinn I</groundspeak:name>
      <groundspeak:placed_by>Joesch</groundspeak:placed_by>
      <groundspeak:owner id="2416870">Joesch</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
      <groundspeak:container>Micro</groundspeak:container>
