    <groundspeak:cache id="2417805" available="False" archived="True" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Student in Cluj - Architecture &amp; Urbanism</groundspeak:name>
      <groundspeak:placed_by>TGQ2011</groundspeak:placed_by>
      <groundspeak:owner id="2863559">zbuciumel</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
