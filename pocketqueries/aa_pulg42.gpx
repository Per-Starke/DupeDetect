    <sym>Geocache Found</sym>
    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="2414037" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>[TGQ2011] Lupa Capitolina</groundspeak:name>
      <groundspeak:placed_by>AirLink &amp; CopyGirl</groundspeak:placed_by>
      <groundspeak:owner id="1686880">AirLink</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
