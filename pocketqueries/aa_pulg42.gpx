    <url>http://www.geocaching.com/seek/cache_details.aspx?guid=41fbad08-f813-41f6-8a03-416b971f215c</url>
    <urlname>Der Abgebrochene am wilden Stadtwald</urlname>
    <sym>Geocache Found</sym>
    <type>Geocache|Multi-cache</type>
    <groundspeak:cache id="2324753" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Der Abgebrochene am wilden Stadtwald</groundspeak:name>
      <groundspeak:placed_by>Oma Eusebia</groundspeak:placed_by>
      <groundspeak:owner id="3641614">Oma Eusebia</groundspeak:owner>
      <groundspeak:type>Multi-cache</groundspeak:type>
