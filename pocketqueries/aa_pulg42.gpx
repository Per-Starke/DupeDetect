    <sym>Geocache Found</sym>
    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="2154096" available="False" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>View on old churches #6: Bob Greek-Catholic Church</groundspeak:name>
      <groundspeak:placed_by>mobellino</groundspeak:placed_by>
      <groundspeak:owner id="2776861">mobellino</groundspeak:owner>
