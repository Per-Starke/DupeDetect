    <desc>Rambla de Mar by Hopehope, Traditional Cache (2/1.5)</desc>
    <url>http://www.geocaching.com/seek/cache_details.aspx?guid=8e519c6a-f271-434e-affe-f99d73e1a529</url>
    <urlname>Rambla de Mar</urlname>
    <sym>Geocache Found</sym>
    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="2683919" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Rambla de Mar</groundspeak:name>
      <groundspeak:placed_by>Hopehope</groundspeak:placed_by>
      <groundspeak:owner id="4310725">Hopehope</groundspeak:owner>
