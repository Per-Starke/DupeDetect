    <groundspeak:cache id="2164888" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Marienkapelle Welldorf</groundspeak:name>
      <groundspeak:placed_by>geissbockfrank</groundspeak:placed_by>
