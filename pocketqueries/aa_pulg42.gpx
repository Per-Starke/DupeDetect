    <groundspeak:cache id="1560254" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Fußballplätze - FC Rasensport - Tetz 1906</groundspeak:name>
      <groundspeak:placed_by>patzwald</groundspeak:placed_by>
      <groundspeak:owner id="324071">Team_Patzwald</groundspeak:owner>
