    <url>http://www.geocaching.com/seek/cache_details.aspx?guid=af3db478-e286-4c31-be20-f71dcae68d28</url>
    <urlname>Hoch über Ilmenau</urlname>
    <sym>Geocache Found</sym>
    <type>Geocache|Traditional Cache</type>
    <groundspeak:cache id="2744550" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Hoch über Ilmenau</groundspeak:name>
      <groundspeak:placed_by>dennistreysa</groundspeak:placed_by>
      <groundspeak:owner id="5142907">dennistreysa</groundspeak:owner>
      <groundspeak:type>Traditional Cache</groundspeak:type>
