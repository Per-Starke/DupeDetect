      <groundspeak:name>Flying Kids</groundspeak:name>
      <groundspeak:placed_by>wowbagger_de</groundspeak:placed_by>
      <groundspeak:owner id="209491">wowbagger_de</groundspeak:owner>
      <groundspeak:type>Multi-cache</groundspeak:type>
