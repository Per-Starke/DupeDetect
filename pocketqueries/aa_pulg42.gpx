    <groundspeak:cache id="290494" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>NIVEA</groundspeak:name>
      <groundspeak:placed_by>Claudia, Hega &amp; Frederik</groundspeak:placed_by>
      <groundspeak:owner id="114637">Hega</groundspeak:owner>
